{
  "presets": [
    "react",
    "es2015",
    "stage-0"
  ],
  "plugins": [
    ["import", { 
      "libraryName": "antd",
      "style": "css"
    }],
    "transform-react-jsx",
    "transform-object-rest-spread",
    "add-module-exports",
    "react-html-attrs",
    "transform-class-properties",
    "transform-decorators-legacy"
  ]
}
